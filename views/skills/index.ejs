<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="stylesheets/style.css">
    <title>Developer Skills</title>
  </head>
  <body>
    <h1>Skills as of <%= time %> </h1>
    <% if(error) { %>
    <h2><%= error %></h2>
    <% } %>
    <ul>
      <% skills.forEach(function(skill) { %>
        
        <li> 
          <span><%= skill.done ? '✅' : '❔' %> </span>  
          <a href="/skills/<%= skill._id %> "><%= skill.text %></a> </li> 
      
      <% }) %>
    </ul>
  </ul>
  <a href="/skills/new" class="btn">ADD NEW SKILL</a>

  </body>
</html>